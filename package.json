{
  "name": "socialshares",
  "version": "1.0.5",
  "description": "Lightweight share buttons for the web.",
  "main": "dist/socialshares.js",
  "scripts": {
    "start": "npm run dev",
    "build": "webpack",
    "build-min": "NODE_ENV=production webpack -p",
    "create-dist": "shjs scripts/create-dist.js",
    "version": "npm run create-dist && git add -A dist",
    "release": "shjs scripts/release.js",
    "dev": "webpack-dev-server --content-base ./build --port 3000 --hot --inline --progress --colors",
    "svgo": "svgo -f ./src/icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socialshares/buttons.git"
  },
  "keywords": [
    "socialshares",
    "social",
    "share",
    "buttons"
  ],
  "author": "Sunny Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/socialshares/buttons/issues"
  },
  "homepage": "https://socialshar.es/",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.22.0",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-cssnext": "^2.4.0",
    "postcss-each": "^0.9.3",
    "postcss-loader": "^0.9.1",
    "prompt": "^1.0.0",
    "shelljs": "^0.7.3",
    "style-loader": "^0.13.0",
    "svg-inline-loader": "^0.6.1",
    "svgo": "^0.6.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "lodash.merge": "^4.4.0"
  }
}
